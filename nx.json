    "shared-utilities-types": {
      "tags": []
    },
    "demo": {
      "tags": []
    },
    "demo-e2e": {
      "tags": [],
      "implicitDependencies": ["demo"]
    },
    "demo-shell": {
      "tags": ["scope:demo", "type:library"]
    },
    "demo-feature-auth": {
      "tags": ["scope:demo", "type:library"]
    },
    "demo-feature-post": {
      "tags": ["scope:demo", "type:library"]
    }
